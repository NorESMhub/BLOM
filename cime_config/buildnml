#! /bin/csh -f

#------------------------------------------------------------------------------
# Get variables from Case XML-files
#------------------------------------------------------------------------------

setenv CASE `./xmlquery CASE --value`
setenv CONFIG_OCN_FILE `./xmlquery CONFIG_OCN_FILE --value`
setenv CASEROOT `./xmlquery CASEROOT --value`
setenv EXEROOT `./xmlquery EXEROOT --value`
setenv OCN_GRID `./xmlquery OCN_GRID --value`
setenv CONFIG_OCN_DIR1 `dirname $CONFIG_OCN_FILE`
setenv NTASKS_OCN `./xmlquery NTASKS_OCN --value` 
setenv DIN_LOC_ROOT `./xmlquery  DIN_LOC_ROOT --value`
setenv RUN_TYPE `./xmlquery RUN_TYPE --value`
setenv CONTINUE_RUN `./xmlquery CONTINUE_RUN --value`
setenv CASEBUILD `./xmlquery CASEBUILD --value`
setenv CCSM_CO2_PPMV `./xmlquery CCSM_CO2_PPMV --value`
setenv OCN_NCPL `./xmlquery OCN_NCPL --value`
setenv BLOM_COUPLING `./xmlquery BLOM_COUPLING --value`
setenv RUNDIR `./xmlquery RUNDIR --value`
setenv BLOM_TRACER_MODULES `./xmlquery BLOM_TRACER_MODULES --value`
setenv BLOM_RIVER_NUTRIENTS `./xmlquery BLOM_RIVER_NUTRIENTS --value`
setenv BLOM_N_DEPOSITION `./xmlquery BLOM_N_DEPOSITION --value`
setenv BLOM_N_DEP_FILE `./xmlquery BLOM_N_DEP_FILE --value`
setenv RUN_STARTDATE `./xmlquery RUN_STARTDATE --value`
echo $CASE ${EXEROOT} $CASEBUILD
cd $EXEROOT/ocn/obj
setenv CONFIG_OCN_DIR $CONFIG_OCN_DIR1/../

#------------------------------------------------------------------------------
# Make dimensions.F
#------------------------------------------------------------------------------

set dimdir = $EXEROOT/ocn/obj/dimensions
mkdir -p $dimdir
set kdm = `cat $CONFIG_OCN_DIR/bld/$OCN_GRID/kdm`
$CONFIG_OCN_DIR/bld/blom_dimensions -n $NTASKS_OCN -k $kdm -d $CONFIG_OCN_DIR/bld/$OCN_GRID || exit -1
set recompile = FALSE
cmp -s dimensions.F $dimdir/dimensions.F || set recompile = TRUE
echo $PWD
echo $recompile
if ($recompile == 'TRUE') then
  mv dimensions.F $dimdir
else
  rm dimensions.F
endif

#------------------------------------------------------------------------------
# Set RUN_TYPE to 'continue' if CONTINUE_RUN equals TRUE
#------------------------------------------------------------------------------

if ($CONTINUE_RUN == TRUE) then
  setenv RUN_TYPE continue
endif

#------------------------------------------------------------------------------
# Generate blom.input_data_list
#------------------------------------------------------------------------------

if      ($OCN_GRID == gx1v5) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20091216
else if ($OCN_GRID == gx1v6) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20101119
else if ($OCN_GRID == gx3v7) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20110921
else if ($OCN_GRID == tnx2v1) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20130206
else if ($OCN_GRID == tnx1.5v1) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20131011
else if ($OCN_GRID == tnx1v1) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20120120
else if ($OCN_GRID == tnx1v3) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20170612
else if ($OCN_GRID == tnx1v4) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20170601
else if ($OCN_GRID == tnx0.25v1) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20130930
else if ($OCN_GRID == tnx0.25v3) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20170623
else if ($OCN_GRID == tnx0.25v4) then
  setenv INPUTPATH $DIN_LOC_ROOT/ocn/blom/$OCN_GRID/20170619
else
  echo "$0 ERROR: Cannot deal with GRID = $OCN_GRID "
  exit -1
endif
echo $CASEBUILD
cat > $CASEBUILD/blom.input_data_list << EOF1
grid_file = $INPUTPATH/grid.nc
ncep_landmask_file = $INPUTPATH/land.sfc.gauss.nc
ncep_height_file = $INPUTPATH/hgt.sfc.nc
ice_clim_file = $INPUTPATH/icec_1968-1996.uf
sst_clim_file = $INPUTPATH/skt_1968-1996.uf
tidal_dissipation_file = $INPUTPATH/tidal_dissipation.nc
meridional_transport_index_file = $INPUTPATH/mertraindex.dat
meridional_transport_basin_file = $INPUTPATH/mertraoceans.dat
section_index_file = $INPUTPATH/secindex.dat
EOF1
if ($RUN_TYPE == startup)  then
cat >> $CASEBUILD/blom.input_data_list << EOF1
inicon_file = $INPUTPATH/inicon.nc
EOF1
endif
set tracers = (`echo $BLOM_TRACER_MODULES`)
set rivnutr = (`echo $BLOM_RIVER_NUTRIENTS`)
set ndep    = (`echo $BLOM_N_DEPOSITION`)
foreach module ($tracers)
  if ($module == ecosys) then
cat >> $CASEBUILD/blom.input_data_list << EOF1
dust_file = $INPUTPATH/INPDUST_mhw.nc
EOF1
    if ($rivnutr == TRUE) then
cat >> $CASEBUILD/blom.input_data_list << EOF1
river_file = $INPUTPATH/river_nutrients_GNEWS2000.nc
EOF1
    endif
    if ($ndep == TRUE) then
cat >> $CASEBUILD/blom.input_data_list << EOF1
n_deposition_file = $INPUTPATH/$BLOM_N_DEP_FILE
EOF1
    endif
  endif
end

#------------------------------------------------------------------------------
# Set namelist variables
#------------------------------------------------------------------------------

# set LIMITS defaults
set ICFILE   = "'inicon.nc'"
set BACLIN   = 1800.
set BATROP   = 36.
set MDV2HI   = 2.
set MDV2LO   = .4
set MDV4HI   = 0.
set MDV4LO   = 0.
set MDC2HI   = 5000.e4
set MDC2LO   = 300.e4
set VSC2HI   = .5
set VSC2LO   = .5
set VSC4HI   = 0.
set VSC4LO   = 0.
set CBAR     = 5.
set CB       = .002
set CWBDTS   = 5.e-5
set CWBDLS   = 25.
set MOMMTH   = "'enscon'"
set EITMTH   = "'gm'"
set EDRITP   = "'large scale'"
