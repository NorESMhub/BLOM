set LVL_ASIZE     = '0, 0, 0'
set SDM_POWAIC    = '0, 0, 2'
set SDM_POWAAL    = '0, 0, 2'
set SDM_POWAPH    = '0, 0, 2'
set SDM_POWAOX    = '0, 0, 2'
