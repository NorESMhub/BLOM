set NIWGF    = 0.
set NIWBF    = .35
set NIWLF    = .5
set SWAMTH   = "'jerlov'"
set JWTYPE   = 3
set CHLOPT   = "'climatology'"
set TRXDAY   = 0.
set SRXDAY   = 0.
set TRXDPT   = 1.
set SRXDPT   = 1.
set TRXLIM   = 1.5
set SRXLIM   = .5
set APTFLX   = .false.
set APSFLX   = .false.
set DITFLX   = .false.
set DISFLX   = .false.
set SRXBAL   = .false.
set SRXSRC   = "'CORE'"
set SMTFRC   = .true.
set SPRFAC   = .false.
set RSTFRQ   =  1
set RSTFMT   =  1
set RSTCMP   =  0 
set IOTYPE   =  0 
# set BGCNML defaults
set ATM_CO2  = $CCSM_CO2_PPMV
if ($rivnutr == TRUE) then
  set DO_RIVINPT = .true.
else
  set DO_RIVINPT   = .false.
endif
if ($ndep == TRUE) then
  set DO_NDEP   = .true.
  set NDEPFNAME = $BLOM_N_DEP_FILE
else
  set DO_NDEP   = .false.
  set NDEPFNAME = "''"
endif
# set DIAPHY defaults 
set GLB_FNAMETAG = "'hd','hm','hy'"  
set GLB_AVEPERIO =  '1,  30, 365'
